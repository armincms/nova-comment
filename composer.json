{
    "name": "armincms/nova-comment",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "laravelista/comments": "~3.6"
    },
    "autoload": {
        "psr-4": {
            "Armincms\\NovaComment\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Armincms\\NovaComment\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
